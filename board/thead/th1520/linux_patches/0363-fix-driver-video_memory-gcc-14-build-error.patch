---
 drivers/soc/xuantie/video_memory/video_memory.c | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/drivers/soc/xuantie/video_memory/video_memory.c b/drivers/soc/xuantie/video_memory/video_memory.c
index b453dcf10772..6e7fa9f6eadd 100644
--- a/drivers/soc/xuantie/video_memory/video_memory.c
+++ b/drivers/soc/xuantie/video_memory/video_memory.c
@@ -1426,9 +1426,9 @@ static long vidalloc_ioctl(struct file *filp, unsigned int cmd, unsigned long ar
     if (_IOC_NR(cmd) > MEMORY_IOC_MAXNR) return EINVAL;
 
     if (_IOC_DIR(cmd) & _IOC_READ)
-        ret = !access_ok(arg, _IOC_SIZE(cmd));
+        ret = !access_ok((void*)arg, _IOC_SIZE(cmd));
     else if (_IOC_DIR(cmd) & _IOC_WRITE)
-        ret = !access_ok(arg, _IOC_SIZE(cmd));
+        ret = !access_ok((void*)arg, _IOC_SIZE(cmd));
     if (ret) return EINVAL;
 
     switch (cmd)
